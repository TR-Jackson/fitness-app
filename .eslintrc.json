{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "indent": [
        "error",
        "tab",
        {
            "SwitchCase": 1
        }
    ],
    "linebreak-style": [
        "error",
        "unix"
    ],
    "quotes": [
        "error",
        "single"
    ],
    "semi": [
        "error",
        "never"
    ]
}

}
